#include <stdio.h>
#include <iostream>
#include <stdlib.h>
#include "charStack.h"

using namespace std;

void charStack::push(char c){
  cout << top << endl;
  data[++top] = c;
  //size++;
  if (size < 10) size++;
  top = top % 10;
}

char charStack::peek(){
  if (size == 0) return base;
  return data[top];
}

char charStack::pop(){
  if (size == 0) return base;
  cout << top << endl;
  char ret = data[top--];
  if (top < 0) top = 9;
  size--;
  return ret;
}

void charStack::setBase(char c){
  base = c;
}
