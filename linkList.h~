struct node{
  int key;
  void* data;
  node *next;
};

class linkedList{
 private:
  node *head, tail;
 public:
  linkedList(){
    head = NULL;
    tail = NULL;
  }
};

void insert(int n, void* d);

void remove(int n);

void* find(int n);
