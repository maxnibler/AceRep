#include <iostream>
#include <string>
#include <stdio.h>
#include <stdlib.h>
#include <fstream>
#include "roll.h"
#include "linkList.h"
#include "hashtable.h"

using namespace std;

int main (/*int argc, char* argv[]*/){
  hashtable hash = hashtable();
  string str = "what";
  
  hash.add(str, &str);
  string* what = static_cast<string*>(hash.get(str));
  cout << *what << endl;
  return 0;
}
